{
  "rewrites": [
    {
      "source": "/((?!chat-widget-embed\\.html|embed-script\\.js).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/chat-widget-embed.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/embed-script.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
