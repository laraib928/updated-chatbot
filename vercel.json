{
    "rewrites": [
      {
        "source": "/((?!chat-widget-embed\\.html|embed-script\\.js|.*\\.(css|js|html|json|png|jpg|svg|ico|woff|woff2)).*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/chat-widget-embed.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Type",
            "value": "text/html"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOWALL"
          }
        ]
      },
      {
        "source": "/embed-script.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          },
          {
            "key": "Content-Type",
            "value": "application/javascript"
          },
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }